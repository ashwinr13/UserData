
<h2> Users </h2>
<table id= "Users">

<thead>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Age</th>
			<th>Email</th>
			<th>Favorite Food</th>
			<th> </th>

		</tr>
</thead>
<tbody>
	<% @users.each do |user| %>
		<tr>
			<td><%= user.firstname %></td>
			<td><%= user.lastname %></td>
			<td><%= user.age %></td>
			<td><%= user.email %></td>
			<td><%= user.favoritefood %></td>
			<td>
				<%= link_to("Show", {:action => 'show', :id => user.id}, :class => 'action show') %>
				<%= link_to("Edit", {:action => 'edit', :id => user.id}, :class => 'action edit') %>
				<%= link_to("Delete", {:action => 'delete', :id => user.id}, :class => 'action delete') %>
			</td>
		</tr>


<% end %>

</tbody>
</table>
<br>
<br>
<button type="button">
<%= link_to("Create User", {:action => 'new'}, :class => 'action new') %>
</button>